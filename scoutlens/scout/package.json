{
  "name": "scout-web3-agent",
  "version": "1.0.0",
  "description": "Scout - Your Personalized Web3 AI Agent. See, Scan, Act. Instantly.",
  "main": "background.js",
  "scripts": {
    "dev": "echo 'Load Scout extension in Chrome at chrome://extensions/ with Developer mode enabled'",
    "build": "echo 'Building Scout extension...' && npm run validate",
    "validate": "node scripts/validate.js",
    "lint": "eslint *.js",
    "format": "prettier --write *.js *.html *.css *.json",
    "zip": "node scripts/zip.js",
    "test": "echo 'Run tests for Scout extension'",
    "clean": "rimraf dist/ *.zip",
    "load": "echo 'Open chrome://extensions/ and load the scout folder as unpacked extension'"
  },
  "keywords": [
    "web3",
    "ai-agent",
    "chrome-extension",
    "ethereum",
    "metamask",
    "defi",
    "tokens",
    "ocr",
    "crypto",
    "manifest-v3"
  ],
  "author": "Scout Team",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "archiver": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/chrome-extension-starter.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/chrome-extension-starter/issues"
  },
  "homepage": "https://github.com/your-username/chrome-extension-starter#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    "Chrome >= 88"
  ]
}
